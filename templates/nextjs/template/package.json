{
  "name": "next-app",
  "version": "2.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "biome check --diagnostic-level=error && eslint . --quiet --concurrency=auto",
    "lint:fix": "biome check --write --diagnostic-level=error && eslint . --quiet --concurrency=auto --fix",
    "lint:css": "stylelint '**/*.css' --quiet",
    "test": "jest --watch",
    "test:ci": "mkdir -p __reports__ && jest --ci --coverage --runInBand --json --outputFile=__reports__/jest-results.json --reporters default --reporters \"github-actions\""
  },
  "dependencies": {
    "@next/bundle-analyzer": "^16.0.3",
    "@sumup-oss/circuit-ui": "^10.0.0",
    "@sumup-oss/design-tokens": "^9.0.0",
    "@sumup-oss/icons": "^5.22.0",
    "@sumup-oss/intl": "^3.3.0",
    "next": "^16.0.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "temporal-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.0.3",
    "@sumup-oss/eslint-plugin-circuit-ui": "^7.0.0",
    "@sumup-oss/foundry": "^9.0.5",
    "@sumup-oss/stylelint-plugin-circuit-ui": "^4.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint-plugin-jest": "^29.11.2",
    "eslint-plugin-testing-library": "^7.15.3",
    "jest": "^30.1.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0"
  }
}
