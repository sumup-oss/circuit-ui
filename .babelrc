{
  "env": {
    "develop": {
      "plugins": [
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ]
      ],
      "presets": ["env", "react", "stage-3"]
    },
    "build": {
      "plugins": [
        "external-helpers",
        "lodash",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ]
      ],
      "presets": [["env", { "modules": false }], "react", "stage-3"]
    },
    "test": {
      "plugins": [
        "lodash",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ]
      ],
      "presets": ["env", "react", "stage-3"]
    },
    "static": {
      "plugins": [
        [
          "emotion",
          {
            "autoLabel": false,
            "hoist": false,
            "sourceMap": false
          }
        ],
        "dynamic-import-node",
        [
          "module-resolver",
          {
            "root": ".",
            "alias": {
              "react-emotion": "./build/stylesheet/lib/custom-styled",
              "^.+svg$": "./build/stylesheet/SvgMock"
            },
            "extensions": [".svg", ".js"]
          }
        ]
      ]
    }
  }
}
