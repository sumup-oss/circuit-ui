{
  "env": {
    "cjs": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ],
        "inline-react-svg"
      ],
      "presets": [["env", { "loose": true }], "react", "stage-3"]
    },
    "es": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ],
        "inline-react-svg"
      ],
      "presets": [
        ["env", { "loose": true, "modules": false }],
        "react",
        "stage-3"
      ]
    },
    "test": {
      "plugins": [
        "transform-class-properties",
        "lodash",
        "transform-object-rest-spread",
        [
          "emotion",
          {
            "autoLabel": true,
            "hoist": true,
            "sourceMap": true
          }
        ]
      ],
      "presets": ["env", "react", "stage-3"]
    },
    "static": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "react",
        "stage-3"
      ],
      "plugins": [
        "lodash",
        "transform-es2015-modules-commonjs",
        "transform-object-rest-spread",
        "transform-class-properties",
        [
          "emotion",
          {
            "autoLabel": false,
            "hoist": false,
            "sourceMap": false
          }
        ],
        "dynamic-import-node",
        [
          "module-resolver",
          {
            "root": ".",
            "alias": {
              "react-emotion": "./build/stylesheet/lib/custom-styled",
              "^.+svg$": "./build/stylesheet/SvgMock"
            },
            "extensions": [".svg", ".js"]
          }
        ]
      ]
    }
  }
}
