{
  "plugins": [
    "lodash",
    "syntax-dynamic-import",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "emotion",
      {
        "autoLabel": true,
        "hoist": true,
        "sourceMap": true
      }
    ]
  ],
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "react",
    "stage-3"
  ],
  "env": {
    "static": {
      "plugins": [
        [
          "emotion",
          {
            "autoLabel": false,
            "hoist": false,
            "sourceMap": false
          }
        ],
        "dynamic-import-node",
        [
          "module-resolver",
          {
            "root": ".",
            "alias": {
              "react-emotion": "./build/stylesheet/lib/custom-styled",
              "^.+svg$": "./build/stylesheet/SvgMock"
            },
            "extensions": [".svg", ".js"]
          }
        ]
      ]
    }
  }
}
