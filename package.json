{
  "name": "ui-components",
  "version": "0.0.1",
  "description": "SumUp's React UI component library",
  "main": "index.js",
  "scripts": {
    "fix:prettier": "prettier --config prettier.config.js --write src/**/*.js && prettier --config prettier.config.js --write stories/*.js",
    "fix:eslint": "eslint --fix --quiet src/**/*.js",
    "fix:stylelint": "stylelint \"src/**/*.scss\" --cache --fix",
    "fix": "npm run fix:prettier; npm run fix:eslint; npm run fix:stylelint",
    "test": "npm run test:lint-js && npm run test:lint-css && npm run test:unit",
    "test:lint-css:watch": "onchange \"src/**/*.scss\" \"src/**/*.scss\" -- npm run test:lint-css",
    "test:lint-css": "stylelint \"src/**/*.scss\" --cache",
    "test:lint-js": "eslint --cache --quiet src/**/*.js",
    "test:lint-js:watch": "onchange \"src/**/*.js\" -- npm run test:lint-js",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "precommit": "npm run fix:prettier && npm test",
    "start": "start-storybook -p 6006",
    "export": "build-storybook -c .storybook -o .out",
    "deploy": "storybook-to-ghpages"
  },
  "lint-staged": {
    "*.js": [
      "npm run fix:prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/ui-components.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sumup/ui-components/issues"
  },
  "homepage": "https://github.com/sumup/ui-components#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/addon-options": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-loader": "^7.1.2",
    "eslint": "^4.6.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "react-svg-loader": "^2.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "expect": "^21.1.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.2.3",
    "onchange": "^3.2.1",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.0",
    "react-addons-test-utils": "^15.6.0",
    "storybook-addon-smart-knobs": "^3.0.1",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.0.1",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "autoprefixer": "^7.1.5",
    "babel-eslint": "^8.0.1",
    "babel-plugin-lodash": "^3.2.11",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^1.9.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "enzyme": "^2.8.0",
    "isomorphic-style-loader": "^4.0.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-object-fit-images": "^1.1.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^15.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2"
  }
}
