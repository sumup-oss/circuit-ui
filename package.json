{
  "name": "sumup-ui",
  "version": "0.0.1",
  "description": "SumUp's React UI component library",
  "main": "index.js",
  "scripts": {
    "fix:prettier": "prettier --config prettier.config.js --write src/**/*.js && prettier --config prettier.config.js --write stories/*.js",
    "fix:eslint": "eslint --fix --quiet src/**/*.js",
    "fix:stylelint": "stylelint \"src/**/*.scss\" --cache --fix",
    "fix": "npm run fix:prettier; npm run fix:eslint; npm run fix:stylelint",
    "test": "npm run test:lint-js && npm run test:lint-css && npm run test:unit",
    "test:lint-css:watch": "onchange \"src/**/*.scss\" \"src/**/*.scss\" -- npm run test:lint-css",
    "test:lint-css": "stylelint \"src/**/*.scss\" --cache",
    "test:lint-js": "eslint --cache --quiet src/**/*.js",
    "test:lint-js:watch": "onchange \"src/**/*.js\" -- npm run test:lint-js",
    "test:unit": "jest --coverage",
    "test:unit:watch": "npm run test:unit:generate-output -- --watch",
    "test:unit:generate-output": "jest --json --outputFile=jest-test-results.json || true",
    "prepush": "npm test",
    "start": "start-storybook -p 6006",
    "prebuild": "npm run test:unit:generate-output",
    "build": "build-storybook -c .storybook -o .out",
    "deploy": "storybook-to-ghpages"
  },
  "lint-staged": {
    "*.js": [
      "npm run fix:prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/sumup-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sumup/sumup-ui/issues"
  },
  "homepage": "https://github.com/sumup/sumup-ui#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.3",
    "@storybook/addon-info": "^3.3.3",
    "@storybook/addon-knobs": "^3.3.3",
    "@storybook/addon-links": "^3.3.3",
    "@storybook/addon-notes": "^3.3.3",
    "@storybook/addon-options": "^3.3.3",
    "@storybook/react": "^3.3.3",
    "@storybook/storybook-deployer": "^2.2.0",
    "babel-loader": "^7.1.2",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^22.0.3",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "lint-staged": "^6.0.0",
    "onchange": "^3.3.0",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.7.0",
    "react-addons-test-utils": "^15.6.2",
    "storybook-addon-smart-knobs": "^3.2.10",
    "stylelint": "^8.4.0",
    "stylelint-config-sass-guidelines": "^4.0.1",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@storybook/addon-jest": "^3.3.3",
    "autoprefixer": "^7.2.4",
    "babel-eslint": "^8.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "bourbon": "^4.3.4",
    "bourbon-neat": "^1.9.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "hoist-non-react-statics": "^2.3.1",
    "isomorphic-style-loader": "^4.0.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.9",
    "postcss-object-fit-images": "^1.1.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "recompose": "^0.26.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1"
  }
}
