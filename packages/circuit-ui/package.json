{
  "name": "@sumup-oss/circuit-ui",
  "version": "10.13.1",
  "description": "SumUp's React UI component library",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./legacy": {
      "types": "./dist/legacy.d.ts",
      "default": "./dist/legacy.js"
    },
    "./experimental": {
      "types": "./dist/experimental.d.ts",
      "default": "./dist/experimental.js"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "default": "./dist/internal.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "typesVersions": {
    "*": {
      "legacy": [
        "./dist/legacy.d.ts"
      ],
      "experimental": [
        "./dist/experimental.d.ts"
      ],
      "internal": [
        "./dist/internal.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "directory": "packages/circuit-ui",
    "url": "git+https://github.com/sumup-oss/circuit-ui.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sumup-oss/circuit-ui/issues"
  },
  "homepage": "https://github.com/sumup-oss/circuit-ui/tree/main/packages/circuit-ui/README.md",
  "scripts": {
    "build": "vite build && tsc -p tsconfig.build.json && rm -rf dist/node_modules",
    "lint": "foundry run eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@nanostores/react": "^1.0.0",
    "nanostores": "^1.0.1"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.4.0",
    "@emotion/jest": "^11.13.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@sumup-oss/design-tokens": "^9.0.0",
    "@sumup-oss/icons": "^5.20.0",
    "@sumup-oss/intl": "^3.1.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.5.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jest-axe": "^10.0.0",
    "mockdate": "^3.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-swipeable": "^7.0.2",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "temporal-polyfill": "^0.3.0",
    "typescript": "^5.9.2",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^6.3.6",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "@emotion/is-prop-valid": "^1.2.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@sumup-oss/design-tokens": ">=9.0.0",
    "@sumup-oss/icons": ">=5.6.0",
    "@sumup-oss/intl": "^3.1.1",
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0",
    "temporal-polyfill": ">= 0.2.0 < 0.4.0"
  },
  "engines": {
    "node": ">=20",
    "typescript": ">=4.1"
  }
}
