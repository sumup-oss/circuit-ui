{
  "name": "@sumup/icons",
  "version": "0.0.0-semantically-released",
  "description": "A collection of icons by SumUp",
  "repository": "git@github.com:sumup-oss/icons.git",
  "author": "Connor BÃ¤r <connor.baer@sumup.com>",
  "license": "Apache-2.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "manifest.json",
    "dist",
    "*.svg"
  ],
  "scripts": {
    "build": "ts-node ./scripts/web.ts",
    "lint": "foundry run eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "yarn lint --fix",
    "lint:ci": "yarn lint --format junit -o __reports__/eslint-results.xml",
    "check-sizes": "./scripts/check-svg-sizes.sh",
    "prerelease": "yarn build && cp web/v1/* .",
    "release": "foundry run semantic-release",
    "postrelease": "rm -f *.svg"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@sumup/foundry": "^3.0.0",
    "@types/babel__core": "^7.1.7",
    "@types/dedent": "^0.7.0",
    "@types/lodash": "^4.14.149",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "dedent": "^0.7.0",
    "lodash": "^4.17.15",
    "svgo": "^1.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": ">=16.0.0 <18.0.0"
  }
}
