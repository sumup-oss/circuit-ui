    "@sumup/design-tokens": "^4.0.0-canary.1",
    "@sumup/icons": "^2.7.0-canary.0",
    "@sumup/intl": "^1.2.0",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.9.2",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
