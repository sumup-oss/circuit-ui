{
  "name": "next-app",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "foundry run eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watch",
    "test:ci": "mkdir -p __reports__ && jest --ci --coverage --runInBand --json --outputFile=__reports__/jest-results.json --reporters default --reporters \"github-actions\""
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.2.5",
    "@sumup/circuit-ui": "^8.0.0",
    "@sumup/design-tokens": "^7.0.0",
    "@sumup/icons": "^3.0.0",
    "@sumup/intl": "^1.5.0",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@sumup/eslint-plugin-circuit-ui": "^4.0.0",
    "@sumup-oss/foundry": "^8.1.1",
    "@sumup/stylelint-plugin-circuit-ui": "^2.0.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^18.19.39",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint-config-next": "^14.2.5",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.5.3",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
