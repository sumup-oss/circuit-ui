{
  "presets": [
    ["@babel/preset-env", { "loose": true }],
    "@babel/preset-react",
    [
      "@emotion/babel-preset-css-prop",
      {
        "autoLabel": false,
        "sourceMap": false,
        "labelFormat": "[filename]--[local]"
      }
    ]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "lodash"
  ],
  "env": {
    "test": {
      "plugins": [
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "lodash",
        "react-docgen"
      ],
      "presets": [
        "@babel/preset-typescript",
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
